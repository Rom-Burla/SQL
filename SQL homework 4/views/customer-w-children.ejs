<%-include('./partials/header.ejs')%>
<link rel="stylesheet" href="./css/style.css">
<script src="./app/index.js" type="module"></script>
<title>העמוד של <%=result[0].name%></title>
<%-include('./partials/navbar.ejs')%>
<img src="data:image/png;base64," alt="segal-logo" class="logo-pic">
<div class="container">
<div class="children-details">
<h1>שלום <%=result[0].name%></h1>
<h2>הילדים שלך:</h2>
<%result.forEach(child=>{%>
    <div class="child-details">
    <h3>שם: <%=child.child_name%></h3>
    <h3>גיל: <%=child.age===1?'שנה':child.age===2?'שנתיים':child.age%></h3>
    </div>
<%})%>
</div>
<h2>הוספת ילד</h2>
<form action="/childReg" method="post">
    <div>
        <label for="">שם הילד</label>
        <input type="text" name="childName">
    </div>
    <div>
        <label for="">גיל הילד</label>
        <input type="text" name="childAge">
    </div>
    <div id="child-form-user-id">
        <label for="">user_id</label>
        <input type="text" name="user_id" value=<%=result[0].id%>>
    </div>
    <button>הוסף</button>
</form>
</div>